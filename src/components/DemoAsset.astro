---
const { app = 'examples', file } = Astro.props;
const host = import.meta.env.PROD
  ? `/flutter/${app}/`
  : 'http://localhost:5555/';
---

<p>
  {
    /\.png/.test(file) ? (
      <img src={`${host}assets/assets/images/${file}`} class="bg-black" />
    ) : (
      <audio src={`${host}assets/assets/audio/${file}`} controls />
    )
  }
</p>
